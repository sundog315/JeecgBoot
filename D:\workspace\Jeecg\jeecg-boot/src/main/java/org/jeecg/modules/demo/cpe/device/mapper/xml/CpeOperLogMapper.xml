<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.demo.cpe.device.mapper.CpeOperLogMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  cpe_oper_log 
		WHERE
			 cpe_id = #{mainId} 
	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.demo.cpe.device.entity.CpeOperLog">
		SELECT * 
		FROM  cpe_oper_log
		WHERE
			 cpe_id = #{mainId} 
	</select>
</mapper>
